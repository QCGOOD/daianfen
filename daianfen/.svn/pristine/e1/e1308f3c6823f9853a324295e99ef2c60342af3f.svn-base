page{
   background:#f2f2f2;
}
/* 去除滚动条 */
::-webkit-scrollbar {
   width: 0;
   height: 0;
   color: transparent;
 }
 .null{
   width:100%;
   text-align: center;
   color:#808080;
 }
 .null image{
   display: block;
   width:450rpx;
   height:400rpx;
   margin:50rpx auto 20rpx;
 }

.reserve-wrap{
   overflow: hidden;
}
.reserve-item{
   background:#fff;
   font-size:26rpx;
   margin-top:30rpx;
}
.reserve-item .reserve-top{
   display: flex;
   justify-content: space-around;
   align-items: center;
   border-bottom:1px solid #f2f2f2;
   font-size:30rpx;
   margin-left:30rpx;
	padding:20rpx 0;
}
.reserve-item .reserve-top .head-img{
   width:66rpx;
   height:66rpx;
   border-radius:50%;
   background:#f2f2f2;
   margin-right:20rpx;
}
.reserve-item .reserve-top .name{
   flex:1;
   text-align: left;
}
.reserve-item .reserve-top .cancel{
   padding-left:5px;
   padding-right:30rpx;
   color:#b80223;
}

.reserve-content{
   font-size:26rpx;
   padding:20rpx 0;
}
.reserve-content .content-item{
   display: flex;
   justify-content: space-around;
   align-items: baseline;
   color:#808080;
   padding:3px 0;
}
.content-item .icon{
   font-size:26rpx;
   color:#999;
   margin-left:30rpx;
   margin-right:20rpx;
}
.content-item .text-l{
   flex:1;
   text-align: left;
}
.content-item .text-r{
   padding-left:5px;
   padding-right:30rpx;
}