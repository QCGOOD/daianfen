page{
   background:#f2f2f2;
}
.detail-section{
   background:#fff;
   font-size:28rpx;
   padding-bottom:30rpx;
   margin-bottom:30rpx;
   overflow:hidden;
}
.detail-item{
   display: flex;
   justify-content: space-around;
   align-items: baseline;
   text-align: left;
   color: #808080;
   margin-top:30rpx;
}
.detail-item .icon{
   font-size:26rpx;
   color:#999;
   padding-left:30rpx;
   padding-right:20rpx;
}
.detail-item .title{
   padding-right:20rpx;
}
.detail-item .value{
   flex:1;
   color:#333;
   padding-right:30rpx;
}

.detail-section.foot{
   /*height:550rpx;*/
   padding:30rpx 0 60rpx;
}
.navigation{
   position: absolute;
   bottom:-60rpx;
   right:30rpx;
   display:inline-block;
   padding:5px;
}
.cancel-btn{
   height:100rpx;
   background:#b80223;
   line-height: 100rpx;
   font-size:34rpx;
   color:#fff;
   margin:60rpx 30rpx 0;
}